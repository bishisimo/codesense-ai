{
  "name": "code-reviewer-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "build": "bun run vite build",
    "dev": "bun run vite --host 0.0.0.0",
    "preview": "bun run vite preview",
    "lint": "bun run eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "bun run vue-tsc --build --force"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "echarts": "^6.0.0",
    "element-plus": "^2.4.4",
    "esbuild": "^0.25.9",
    "markdown-it": "^14.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.4.19",
    "vue-tsc": "^1.8.25"
  }
